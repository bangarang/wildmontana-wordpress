{% extends contentOnly ? 'templates/_contentOnly.twig' : 'templates/_document.twig' %}

{% block content %}
  {{ renderComponent('GridPostsArchive', { posts: posts }) }}
  <section>
    {{ renderComponent('BlockForm', {'contentHtml': '<h2>' ~ __('Stay Connected') ~ '</h2>','gravity_form_id': 1, 'options': {'theme': 'themeYellow', 'columns': 1}}) }}
  </section>
  {% for component in post.meta('pageComponents') %}
    {{ renderComponent(component) }}
  {% endfor %}
{% endblock %}
