<div class="flyntComponent componentSpacing topo-wrapper {{ options.theme }}" is="flynt-block-form">
  <div class="container">
    <div class="row">
      {% if options.columns == 1 %}
        <div class="col">
          <div class="content content--{{ textAlignment }}">
            {{ contentHtml|e('wp_kses_post') }}
          </div>
          {% filter shortcodes %}
            [gravityforms id="{{gravity_form_id}}" title="false" ajax="true]
          {% endfilter %}
        </div>
      {% else %}
